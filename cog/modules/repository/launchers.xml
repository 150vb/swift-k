<project name="Launchers" default="create" basedir=".">
	<target name="create">

	  <ant antfile="${main.buildfile}" target="launcher">
		<property name="launcher-name" value="cog-repository"/>
		<property name="class-name"  value="org.globus.cog.repository.util.RepositoryConsole"/>
	  </ant>
          <ant antfile="${main.buildfile}" target="launcher">
		<property name="launcher-name" value="cog-repository-admin"/>
		<property name="class-name"  value="org.globus.cog.repository.util.RepositoryAdminConsole"/>
	  </ant>
          <copy file="./repository-create-db" tofile="./dist/repository-1.0/bin/repository-create-db"/>         
	</target>
</project>
