<project name="Launchers" default="create" basedir=".">
  <target name="create">
	<ant antfile="${main.buildfile}" target="launcher">
		<property name="launcher-name" 
                          value="cog-graph-editor"/>
		<property name="class-name" 
                          value="org.globus.cog.gui.grapheditor.Launcher"/>
	</ant>
  </target>
  <target name="webstart">
	<ant antfile="${main.buildfile}" target="webstart.launcher">
		<property name="launcher-name" 
                          value="cog-graph-editor"/>
		<property name="class-name" 
                          value="org.globus.cog.gui.grapheditor.Launcher"/>
		<property name="display-name" 
                          value="Graph Editor"/>
	</ant>
  </target>
</project>
