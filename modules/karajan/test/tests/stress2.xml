<karajan>
	<map>
		<map:entry key="name" value="another stress test"/>
		<map:entry key="test">
			<element arguments="">
				<element name="bla" arguments="a, b">
					<quotient>
						<variable>a</variable>
						<variable>b</variable>
					</quotient>
				</element>
				<sum>
					<for name="i">
						<range from="1" to="20"/>
						
						<parallelFor name="j">
							<range from="1" to="500"/>
							
							<bla>
								<parallel>
									<sum>
										<number>2</number>
										<number>2</number>
									</sum>
									<sum>
										<number>1</number>
										<number>1</number>
									</sum>
								</parallel>
							</bla>
						</parallelFor>
						
						<echo message="." nl="false"/>
					</for>
				</sum>
				<echo/>
			</element>
		</map:entry>
		<map:entry key="expected-result">
			<product>
				<number>20</number>
				<number>500</number>
				<number>2</number>
			</product>
		</map:entry>
		<map:entry key="comparison" value="{math_comparison}"/>
	</map>
</karajan>