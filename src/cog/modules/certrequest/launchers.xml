<project name="Launchers" default="create" basedir=".">
	<target name="create">
		<ant antfile="${main.buildfile}" target="launcher">
			<property name="launcher-name" value="grid-cert-request"/>
			<property name="class-name" value="org.globus.cog.security.certrequest.GridCertRequest"/>
		</ant>
	</target>
	<target name="webstart"/>
</project>
