<program xmlns="http://ci.uchicago.edu/swift/2009/02/swiftscript"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:xs="http://www.w3.org/2001/XMLSchema">
  
  <types>
     	<type>
     		<typename>file</typename>
     		<typealias>string</typealias>
     		<typestructure></typestructure>
     	</type>   
  </types>  
  <procedure name="echo">

      <output 
    name="f" type="file"

     xsi:nil="true" />

      <input 
    name="i" type="int"

     xsi:nil="true" />
    <binding>
      <application src="line 4">
        <executable>echo</executable>
        <stdout><function name="filename">
          <variableReference>f</variableReference></function></stdout><variableReference>i</variableReference>  </application>
    </binding>
  </procedure>
  <variable name="greetings" type="int" xsi:nil="true"/>
  <variable name="hw" type="file" xsi:nil="true"/>
  <call proc="echo" src="line 8">
    <output><variableReference>hw</variableReference></output>
    <input><variableReference>greetings</variableReference></input>
  </call>
</program>
