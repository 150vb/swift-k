<program xmlns="http://ci.uchicago.edu/swift/2007/07/swiftscript"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <variable name="s" type="int" xsi:nil="true"/>
  <variable name="a" type="int" xsi:nil="true"/>
  <call proc="echo">
    <output><variableReference>a</variableReference></output>
    <input><variableReference>s</variableReference></input>
  </call>
</program>
