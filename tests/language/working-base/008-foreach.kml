<project>
  <import file="sys.xml"/>
  <import file="scheduler.xml"/>
  <import file="rlog.xml"/>
  <import file="vdl.k"/>
  <set name="s">
    <vdl:new type="footype" dbgname="s">
      <vdl:mapping descriptor="concurrent_mapper">
        <vdl:parameter name="prefix" value="s-0fc1bae2-e929-4e04-a90f-9e525983436d"/>
      </vdl:mapping>
    </vdl:new>
  </set>
  <restartLog>
  	<vdl:pre/>
  	<vdl:mains>
		<vdl:mainp>
		    <parallel>
    		  <parallelFor name="$">
    		    <vdl:getarrayfieldvalue path="" var="{sa}"/>
    		      <set names="$$, s">
    		        <each items="{$}"/>
    		      </set>

    		      <parallel>
    		        <echo>
    		          <parallel>
    		            <variable>s</variable>
    		          </parallel>
    		        </echo>
    		      </parallel>
    		  </parallelFor>
	    	</parallel>
		</vdl:mainp>
	</vdl:mains>
	<vdl:post/>
  </restartLog>  
</project>
