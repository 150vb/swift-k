<project>
  <import file="sys.xml"/>
  <import file="scheduler.xml"/>
  <import file="rlog.xml"/>
  <import file="vdl.k"/>
  <restartLog>
  	<vdl:pre/>
  	<vdl:mains>
		<vdl:mainp>
		    <parallel>
    		  <parallelFor name="$">
    		    <vdl:getarrayfieldvalue path="" var="{zdata}"/>
    		      <set names="$$, s">
    		        <each items="{$}"/>
    		      </set>

    		      <parallel>
    		        <echo>
    		          <parallel>
    		            <variable>a</variable>
    		            <variable>s</variable>
    		          </parallel>
    		        </echo>
    		      </parallel>
    		  </parallelFor>
	    	</parallel>
		</vdl:mainp>
	</vdl:mains>
	<vdl:post/>
  </restartLog>  
</project>
