<program xmlns="http://ci.uchicago.edu/swift/2007/07/swiftscript"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <variable name="sa" type="int[]" xsi:nil="true"/>
  <foreach var="s"  src="line 2">
  <in><variableReference>sa</variableReference></in>
  <body><variable name="a" type="int" xsi:nil="true"/>
    <call proc="echo" src="line 2">
      <output><variableReference>a</variableReference></output>
      <input><variableReference>s</variableReference></input>
    </call></body>
  </foreach>
</program>
