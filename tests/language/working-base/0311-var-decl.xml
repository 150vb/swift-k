<program xmlns="http://ci.uchicago.edu/swift/2007/07/swiftscript"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <variable name="x" type="int" xsi:nil="true"/>
  <variable name="y" type="int"><integerConstant>2</integerConstant></variable>
  <variable name="s" type="String"><stringConstant>hello</stringConstant></variable>
  <variable name="f" type="float" isArray="true">
    <array>
      <floatConstant>1.0</floatConstant><floatConstant>2.0</floatConstant><floatConstant>3.0</floatConstant>
    </array>
  </variable>
  <variable name="p" type="int" isArray="true">
    <range>
      <integerConstant>1</integerConstant>
      <integerConstant>9</integerConstant>
      <integerConstant>2</integerConstant>
    </range>
  </variable>
</program>
