<project>
  <import file="sys.xml"/>
  <import file="scheduler.xml"/>
  <import file="rlog.xml"/>
  <import file="vdl.k"/>
  <set name="img1">
    <vdl:new type="Image" dbgname="img1">
      <vdl:mapping descriptor="image_mapper">
        <vdl:parameter name="location">
          <string>/home/archive/images/image1.jpg</string>
        </vdl:parameter>
      </vdl:mapping>
    </vdl:new>
  </set>
  <set name="img2">
    <vdl:new type="Image" dbgname="img2">
      <vdl:mapping descriptor="simple_mapper">
        <vdl:parameter name="prefix">
          <vdl:filename var="{img1}"/>
        </vdl:parameter>
        <vdl:parameter name="suffix">
          <string>.2</string>
        </vdl:parameter>
      </vdl:mapping>
    </vdl:new>
  </set>
</project>
