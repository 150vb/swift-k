<project>
  <import file="sys.xml"/>
  <import file="scheduler.xml"/>
  <import file="rlog.xml"/>
  <import file="vdl.k"/>
  <set name="t">
    <vdl:new type="string" dbgname="t">
      <vdl:mapping descriptor="concurrent_mapper">
        <vdl:parameter name="prefix" value="t-11aee53e-0acd-4810-b0f7-9c5923420f9a"/>
      </vdl:mapping>
    </vdl:new>
  </set>
  <restartLog>
  	<vdl:pre/>
  	<vdl:mains>
		<vdl:mainp>
		    <parallel>
    		  <parallelFor name="$">
    		    <vdl:getarrayfieldvalue path="" var="{subjectNo}"/>
    		      <set names="j, t">
    		        <each items="{$}"/>
    		      </set>
    		      <set name="s">
    		        <vdl:new type="string" dbgname="s">
    		          <vdl:mapping descriptor="concurrent_mapper">
    		            <vdl:parameter name="prefix" value="s-e1bd35da-1212-4400-a28b-cf527e1e634c"/>
    		          </vdl:mapping>
    		        </vdl:new>
    		      </set>
    		      <parallel>
    		        <parallelFor name="$">
    		          <vdl:getarrayfieldvalue path="" var="{trialTypes}"/>
    		            <set names="i, s">
    		              <each items="{$}"/>
    		            </set>
    		            <vdl:setfieldvalue path="[i * j]" var="{outputs}" value="3"/>
    		        </parallelFor>
    		      </parallel>
    		  </parallelFor>
	    	</parallel>
		</vdl:mainp>
	</vdl:mains>
	<vdl:post/>
  </restartLog>  
</project>
