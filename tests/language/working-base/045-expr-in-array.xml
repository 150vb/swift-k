<program xmlns="http://ci.uchicago.edu/swift/2007/07/swiftscript"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <variable name="subjectNo" type="int[]" xsi:nil="true"/>
  <variable name="trialTypes" type="int[]" xsi:nil="true"/>
  <variable name="outputs" type="int[]" xsi:nil="true"/>
  <foreach var="t"  indexVar="j">
  <in><variableReference>subjectNo</variableReference></in>
  <body><foreach var="s"  indexVar="i">
    <in><variableReference>trialTypes</variableReference></in>
    <body><assign>
       <arraySubscript>
        <variableReference>outputs</variableReference>
        <arith op="*">
          <variableReference>i</variableReference>
          <variableReference>j</variableReference>
        </arith>
       </arraySubscript>
       <integerConstant>3</integerConstant>
      </assign></body>
    </foreach></body>
  </foreach>
</program>
