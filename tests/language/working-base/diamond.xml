  </procedure>
  <procedure name="combine">

      <output 
    name="f3" type="file"

     xsi:nil="true" />

      <input 
    name="f1" type="file"

     xsi:nil="true" />

      <input 
    name="f2" type="file"

     xsi:nil="true" />
    <binding>
      <application>
        <executable>combine</executable>
        <stringConstant>-aBOTTOM -T4</stringConstant><stringConstant>-i</stringConstant><function name="filename">
          <variableReference>f1</variableReference></function><function name="filename">
          <variableReference>f2</variableReference></function><stringConstant>-o</stringConstant><function name="filename">
          <variableReference>f3</variableReference></function>  </application>
    </binding>
  </procedure>
  <procedure name="diamond">

      <output 
    name="fd" type="file"

     xsi:nil="true" />

      <input 
    name="p1" type="float"

     xsi:nil="true" />

      <input 
    name="p2" type="float"

     xsi:nil="true" />
    <variable name="fa" type="file" xsi:nil="true"/>
    <variable name="fb" type="file" xsi:nil="true"/>
    <variable name="fc" type="file" xsi:nil="true"/>
    <call proc="generate" src="line 26">
