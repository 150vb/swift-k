<program xmlns="http://ci.uchicago.edu/swift/2009/02/swiftscript"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:xs="http://www.w3.org/2001/XMLSchema">
  
  <types>
     	<type>
     		<typename>messagefile</typename>
     		<typealias>string</typealias>
     		<typestructure></typestructure>
     	</type>   
  </types>  
  <procedure name="greeting">

      <output 
    name="t" type="messagefile"

     xsi:nil="true" />

      <input 
    name="s" type="string"

     xsi:nil="true" />
    <binding>
      <application src="line 4">
        <executable>echo</executable>
        <stdout><function name="filename">
          <variableReference>t</variableReference></function></stdout><variableReference>s</variableReference>  </application>
    </binding>
  </procedure>
  <dataset name="outfile" type="messagefile">
    <file name="hello20.txt"/>
  </dataset>
  <variable name="morning" type="boolean" xsi:nil="true"/>
  <assign src="line 13">
   <variableReference>morning</variableReference>
   <booleanConstant>true</booleanConstant>
  </assign>
  <if>
    <variableReference>morning</variableReference>
    <then>
      <call proc="greeting" src="line 14">
        <output><variableReference>outfile</variableReference></output>
        <input><stringConstant>good morning</stringConstant></input>
      </call>
    </then>
    <else>
      <call proc="greeting" src="line 16">
        <output><variableReference>outfile</variableReference></output>
        <input><stringConstant>good afternoon</stringConstant></input>
      </call>
    </else></if>
</program>
