<program xmlns="http://ci.uchicago.edu/swift/2007/07/swiftscript"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <types>
    <xs:schema targetNamespace="http://ci.uchicago.edu/swift/2007/07/swiftscript">
     <xs:simpleType name="messagefile">
       <xs:restriction base="string"/>
     </xs:simpleType>
    </xs:schema>
  </types>
  <procedure name="greeting">

      <output 
    name="t" type="messagefile"

     xsi:nil="true" />

      <input 
    name="s" type="string[]"

     xsi:nil="true" />
    <binding>
      <application>
        <executable>echo</executable>
        <stdout><function name="filename">
          <variableReference>t</variableReference></function></stdout><arraySubscript>
         <variableReference>s</variableReference>
         <integerConstant>0</integerConstant>
        </arraySubscript><arraySubscript>
         <variableReference>s</variableReference>
         <integerConstant>1</integerConstant>
        </arraySubscript><arraySubscript>
         <variableReference>s</variableReference>
         <integerConstant>2</integerConstant>
        </arraySubscript>  </application>
    </binding>
  </procedure>
  <dataset name="outfile" type="messagefile">
    <file name="q5out.txt"/>
  </dataset>
  <variable name="words" type="string[]">
    <array>
      <stringConstant>how</stringConstant><stringConstant>are</stringConstant><stringConstant>you</stringConstant>
    </array>
  </variable>
  <call proc="greeting">
    <output><variableReference>outfile</variableReference></output>
    <input><variableReference>words</variableReference></input>
  </call>
</program>
