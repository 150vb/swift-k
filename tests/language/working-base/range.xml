<program xmlns="http://ci.uchicago.edu/swift/2007/07/swiftscript"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <variable name="nums" type="int[]">
    <range>
      <integerConstant>0</integerConstant>
      <integerConstant>20</integerConstant>
      <integerConstant>2</integerConstant>
    </range>
  </variable>
  <foreach var="num" >
  <in><variableReference>nums</variableReference></in>
  <body><call proc="print">
      <input><variableReference>num</variableReference></input>
    </call></body>
  </foreach>
</program>
